int buttonState = 0;
int sw = 2;
int led = 13;
void setup() {
  // put your setup code here, to run once:
   pinMode(sw,INPUT);
   pinMode(led,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
   buttonState = digitalRead(sw);
   if(buttonState == HIGH)
   {
    digitalWrite(led,HIGH);
   }
   else{
    digitalWrite(led,LOW);
   }
}